{
    "version": 2,
    "builds": [
        {
            "src": "another_social_app/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.8.10"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(?!media).*",
            "dest": "/another_social_app/wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "another_social_app/wsgi.py"
        }

    ]

}
